{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Node - Current File",
      "type": "node",
      "request": "launch",
      "program": "${file}",
      "runtimeArgs": [
        "--loader",
        "tsx/esm"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "base-frame - debug - msh-env",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/packages/base-frame/src/index.ts",
